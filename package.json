{
  "name": "@voxgig/vue2-medium-editor",
  "version": "2.0.0",
  "description": "A VueJS plugin for medium-editor",
  "main": "index.js",
  "scripts": {
    "test": "node index.js",
    "clean": "rm -rf node_modules package-lock.json yarn.lock dist",
    "build": "webpack",
    "build-production": "NODE_ENV=production webpack",
    "reset": "npm run clean && npm i && npm test",
    "repo-tag": "REPO_VERSION=`node -e \"console.log(require('./package').version)\"`; echo TAG: v$REPO_VERSION && git commit -a -m v$REPO_VERSION && git push && git tag v$REPO_VERSION && git push --tags;",
    "repo-publish": "npm run clean && npm i && npm run build && npm run build-production && npm run repo-tag && npm publish  --access public --registry http://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FranzSkuffka/vue-medium-editor.git"
  },
  "keywords": [
    "vue",
    "js",
    "medium",
    "editor",
    "directive"
  ],
  "author": "Jan Wirth <contact@jan-wirth.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FranzSkuffka/vue-medium-editor/issues"
  },
  "homepage": "https://github.com/FranzSkuffka/vue-medium-editor#readme",
  "dependencies": {
    "@voxgig/medium-editor": "^6.0.0",
    "webpack-cli": "^3.1.2"
  },
  "devDependencies": {
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "babili": "^0.1.2",
    "esm": "^3.2.25",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "webpack": "^2.6.1"
  }
}
